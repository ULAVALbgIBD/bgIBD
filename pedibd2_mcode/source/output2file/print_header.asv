

function print_header(fid, option)

fprintf(fid, '\n');
if( option(3) == 4 )
    fprintf(fid, '*****              dbFamily              *****\n');
else
    fprintf(fid, '*****        generated by ped-ibd        *****\n');
end
fprintf(fid, '*****        %s        *****\n\n', datestr(now));
fprintf(fid, '\n');

end


